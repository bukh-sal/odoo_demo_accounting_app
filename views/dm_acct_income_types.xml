<odoo>
    <record id="dm_income_types_action" model="ir.actions.act_window">
        <field name="name">Income Types - DM</field>
        <field name="res_model">dm.accounting.income.type</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    <record id="dm_income_types_list" model="ir.ui.view">
        <field name="name">Income Types List</field>
        <field name="model">dm.accounting.income.type</field>
        <field name="arch" type="xml">
            <tree string="Properties">
                <field name="name"/>
            </tree>
        </field>
    </record>
    
    
    <record id="dm_account_form_view" model="ir.ui.view">
        <field name="name">Income Type Form</field>
        <field name="model">dm.accounting.income.type</field>
        <field name="arch" type="xml">
            <form string="Properties">
                
                <sheet string="General Information">
                    <group string="General Information">
                        <field name="name"/>
                    </group>
                </sheet>

            </form>
        </field>
    </record>
</odoo>